    ]
  },
  {
    "page_number": 2,
    "bbox": [
      595.85,
      48.08,
      601.89,
      90.4
    ],
    "text": "II",
    "fonts": [
      "Helvetica",
      "Helvetica-Oblique"
    ],
    "sizes": [
      19.799999237060547,
      20.100000381469727
    ]
