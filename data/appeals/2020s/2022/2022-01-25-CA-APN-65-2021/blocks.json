    "fonts": [
      "Calibri"
    ],
    "sizes": [
      12.960000038146973
    ]
  },
  {
    "page_number": 4,
    "bbox": [
      72.02,
      360.53,
      123.31,
      373.49
    ],
    "text": "or death.",
    "fonts": [
      "Calibri"
    ],
    "sizes": [
      12.960000038146973
    ]
  },
  {
    "page_number": 4,
    "bbox": [
      72.02,
      396.29,
      542.5,
      409.25
    ],
    "text": "Therefore the severity of the offence with which the suspect has been charged with,",
    "fonts": [
      "Calibri"
    ],
    "sizes": [
      12.960000038146973
    ]
  },
  {
    "page_number": 4,
    "bbox": [
      72.02,
      420.17,
      542.46,
      433.13
    ],
