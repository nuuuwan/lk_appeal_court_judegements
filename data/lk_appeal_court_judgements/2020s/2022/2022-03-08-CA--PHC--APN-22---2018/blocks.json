    "bbox": [
      85.58,
      728.23,
      332.45,
      756.04
    ],
    "text": "Page 6 of 8",
    "fonts": [
      "BookmanOldStyle",
      "Calibri",
      "Calibri-Bold"
    ],
    "sizes": [
      9.0,
      12.0
    ]
  },
