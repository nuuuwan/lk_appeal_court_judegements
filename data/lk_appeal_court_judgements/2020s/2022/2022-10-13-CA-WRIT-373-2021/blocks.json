    "fonts": [
      "Calisto MT"
    ],
    "sizes": [
      6.960000038146973,
      12.0
    ]
  },
  {
    "page_number": 2,
    "bbox": [
      72.02,
      670.96,
      526.43,
      684.58
    ],
    "text": "about issuing route permits outside the tender procedures by the 1st Respondent Authority",
