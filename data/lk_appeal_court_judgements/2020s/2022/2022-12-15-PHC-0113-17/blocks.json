    "fonts": [
      "TrebuchetMS"
    ],
    "sizes": [
      12.0
    ]
  },
  {
    "page_number": 4,
    "bbox": [
      72.02,
      516.08,
      169.11,
      529.81
    ],
    "text": "is not necessary.",
    "fonts": [
      "TrebuchetMS"
    ],
    "sizes": [
      12.0
    ]
  },
