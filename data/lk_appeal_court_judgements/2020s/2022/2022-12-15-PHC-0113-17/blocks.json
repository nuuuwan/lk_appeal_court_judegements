    "fonts": [
      "Trebuchet-BoldItalic",
      "TrebuchetMS",
      "TrebuchetMS-Italic"
    ],
    "sizes": [
      11.039999961853027
    ]
  },
  {
    "page_number": 5,
    "bbox": [
