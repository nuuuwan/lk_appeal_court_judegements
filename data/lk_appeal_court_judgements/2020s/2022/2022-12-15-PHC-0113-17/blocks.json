    "fonts": [
      "Trebuchet-BoldItalic",
      "TrebuchetMS"
    ],
    "sizes": [
      11.039999961853027
    ]
  },
  {
    "page_number": 5,
    "bbox": [
      72.02,
      461.54,
      543.37,
      474.17
    ],
