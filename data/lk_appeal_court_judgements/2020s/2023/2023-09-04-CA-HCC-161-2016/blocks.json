    "sizes": [
      11.039999961853027
    ]
  },
  {
    "page_number": 4,
    "bbox": [
      72.02,
      399.56,
      122.61,
      412.6
    ],
    "text": "integrity.",
    "fonts": [
      "Century"
    ],
    "sizes": [
      11.039999961853027
    ]
  },
  {
    "page_number": 4,
    "bbox": [
      72.02,
      429.32,
