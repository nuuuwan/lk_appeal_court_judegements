    "fonts": [
      "Calibri"
    ],
    "sizes": [
      12.960000038146973
    ]
  },
  {
    "page_number": 3,
    "bbox": [
      76.58,
      728.23,
      318.89,
      756.04
    ],
    "text": "Page 4 of 6",
    "fonts": [
      "BookmanOldStyle",
      "Calibri",
      "Calibri-Bold"
    ],
    "sizes": [
      9.0,
