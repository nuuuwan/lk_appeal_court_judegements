    "fonts": [
      "Calibri"
    ],
    "sizes": [
      12.960000038146973
    ]
  },
  {
    "page_number": 5,
    "bbox": [
      103.58,
      527.35,
      507.0,
      540.31
    ],
    "text": "the fundamental obligation namely fair trial. In R.H.M.S. Premathunga alias",
    "fonts": [
      "Calibri",
      "Calibri-Bold"
    ],
    "sizes": [
      12.960000038146973
    ]
  },
  {
    "page_number": 5,
    "bbox": [
      103.58,
      551.23,
      506.96,
      564.19
    ],
