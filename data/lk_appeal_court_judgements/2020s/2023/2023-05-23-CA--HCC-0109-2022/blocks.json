    "fonts": [
      "Calibri-Bold"
    ],
    "sizes": [
      12.960000038146973
    ]
  },
  {
    "page_number": 6,
    "bbox": [
      108.02,
      728.23,
      332.33,
      756.04
    ],
    "text": "Page 7 of 8",
