    ]
  },
  {
    "page_number": 6,
    "bbox": [
      108.02,
      253.37,
      511.17,
      266.33
    ],
    "text": "is more than one year which is in violation of section 174(1) of the CPC",
    "fonts": [
      "Calibri-Bold"
    ],
    "sizes": [
      12.960000038146973
    ]
  },
  {
    "page_number": 6,
    "bbox": [
      108.02,
      277.13,
      236.14,
      290.09
    ],
