    "fonts": [
      "Calibri-Bold"
    ],
    "sizes": [
      12.960000038146973
    ]
  },
  {
    "page_number": 4,
    "bbox": [
      103.58,
      384.29,
      156.91,
      397.25
    ],
    "text": "witness.",
    "fonts": [
      "Calibri",
      "Calibri-Bold"
    ],
    "sizes": [
