    "fonts": [
      "Calibri"
    ],
    "sizes": [
      13.199999809265137
    ]
  },
  {
    "page_number": 2,
    "bbox": [
      115.32,
      420.02,
      315.88,
      433.22
    ],
    "text": "circumstance by the Court of Appeal.",
    "fonts": [
      "Calibri"
    ],
    "sizes": [
      13.199999809265137
