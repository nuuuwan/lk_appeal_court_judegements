    "fonts": [
      "BookmanOldStyle"
    ],
    "sizes": [
      12.0
    ]
  },
  {
    "page_number": 4,
    "bbox": [
      70.94,
      324.51,
      528.13,
      338.51
    ],
    "text": "justice. In addition, there must be exceptional circumstances that warrant",
    "fonts": [
      "BookmanOldStyle"
    ],
    "sizes": [
