    "fonts": [
      "Helvetica"
    ],
    "sizes": [
      11.600000381469727,
      11.693077087402344
    ]
  },
  {
    "page_number": 3,
    "bbox": [
      65.01,
      411.59,
      520.37,
      427.54
    ],
