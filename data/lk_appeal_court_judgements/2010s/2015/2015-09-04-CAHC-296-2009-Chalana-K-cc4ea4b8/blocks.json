    "fonts": [
      "Times-Roman"
    ],
    "sizes": [
      11.600000381469727,
      11.642721176147461,
      11.78054428100586
    ]
  },
  {
    "page_number": 3,
    "bbox": [
      67.38,
      510.25,
      579.87,
      525.73
    ],
    "text": "Kumara he was a member of the team led by IP Tennakoon but not assisted IP Tennakoon for the arrest",
    "fonts": [
      "Times-Roman"
    ],
    "sizes": [
      11.600000381469727,
      11.78054428100586
    ]
  },
  {
    "page_number": 3,
    "bbox": [
      67.28,
      531.14,
      581.63,
      546.61
    ],
