    "fonts": [
      "Times-Roman"
    ],
    "sizes": [
      11.690596580505371,
      11.699999809265137
    ]
  },
  {
    "page_number": 2,
    "bbox": [
      65.43,
      495.04,
      564.53,
      510.65
    ],
